<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DeveloperPreference">
        <attr name="twitterHandle" format="string" />
        <attr name="donateLink" format="string" />
        <attr name="avatar" format="reference" />
    </declare-styleable>
    <declare-styleable name="SeekBarPreference">
        <attr name="seekbar_max" format="integer" />
        <attr name="seekbar_min" format="integer" />
        <attr name="seekbar_interval" format="integer" />
        <attr name="seekbar_default_value" format="integer" />
    </declare-styleable>
</resources>
